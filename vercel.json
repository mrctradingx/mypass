{
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" }, // Phục vụ các file tĩnh (JS, CSS, v.v.)
    { "src": "/manifest.json", "dest": "/manifest.json" }, // Phục vụ manifest.json
    { "src": "/favicon.ico", "dest": "/favicon.ico" }, // Phục vụ favicon.ico
    { "src": "/(.*)", "dest": "/index.html" } // Hỗ trợ SPA: tất cả route trả về index.html
  ]
}